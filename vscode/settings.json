{
  "[css]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[handlebars]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[html]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[javascript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[javascriptreact]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[json]": { "editor.defaultFormatter": "vscode.json-language-features" },
  "[jsonc]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[markdown]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[python]": { "editor.formatOnType": true },
  "[typescript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[typescriptreact]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "accessibility.voice.speechLanguage": "en-US",
  "cSpell.enabled": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.renderSideBySide": true,
  "editor.accessibilitySupport": "off",
  "editor.autoClosingBrackets": "never",
  "editor.codeActionsOnSave": { "source.organizeImports": "explicit" },
  "editor.fontFamily": "FiraCode NF, Consolas, 'Courier New', monospace",
  "editor.fontSize": 13,
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "editor.lineNumbers": "relative",
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "none",
  "editor.smoothScrolling": true,
  "editor.tabSize": 2,
  "editor.useTabStops": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.experimental.affinity": { "vscodevim.vim": 1 },
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "never",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "prettier.printWidth": 160,
  "remote.autoForwardPortsSource": "hybrid",
  "security.promptForRemoteFileProtocolHandling": false,
  "security.workspace.trust.untrustedFiles": "open",
  "settingsSync.ignoredSettings": ["editor.fontFamily", "terminal.integrated.fontFamily"],
  "settingsSync.keybindingsPerPlatform": false,
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.defaultProfile.windows": "Command Prompt",
  "terminal.integrated.fontFamily": "FiraCode Nerd Font",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.scrollback": 10000,
  "typescript.tsserver.maxTsServerMemory": 8096,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vim.easymotion": true,
  "vim.foldfix": true,
  "vim.highlightedyank.duration": 100,
  "vim.highlightedyank.enable": true,
  "vim.leader": " ",
  "vim.sneak": true,
  "vim.sneakReplacesF": true,
  "workbench.editor.showTabs": "single",
  "workbench.iconTheme": "vscode-icons",
  "workbench.startupEditor": "none"
}
